{
  "name": "check-my-diary",
  "version": "0.0.1",
  "description": "HMPPS check my diary",
  "repository": "git@github.com:ministryofjustice/check-my-diary.git",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm run build && DEBUG=gov-starter-server* nodemon server.js | bunyan -o short",
    "start-feature": "NODE_ENV=development node server.js | bunyan -o short",
    "compile-sass": "sass --no-source-map --load-path=. ./assets/sass/application.sass:./assets/stylesheets/application.css ./assets/sass/application-ie8.sass:./assets/stylesheets/application-ie8.css --style compressed",
    "watch-sass": "npm run compile-sass -- --watch",
    "build": "npm run compile-sass",
    "clean": "rm -rf node_modules .port.tmp *.log build uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "TZ=UTC jest --config=jest.config.json",
    "int-test": "cypress run",
    "int-test-ui": "cypress open",
    "migrate": "knex migrate:latest",
    "record-build-info": "node ./bin/record-build-info",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "test-mocks": "jest --config=jest.config.integration.json",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.*",
    "npm": ">=7.11.0"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^1.3.1",
    "agentkeepalive": "^4.1.4",
    "applicationinsights": "^2.3.1",
    "audit-ci": "^5.1.2",
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^2.0.0",
    "csurf": "^1.10.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "ejs": "3.1.6",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "express-validator": "^6.14.0",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^4.0.1",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "ip-range-check": "^0.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "knex": "^1.0.6",
    "moment": "^2.29.2",
    "morgan": "^1.10.0",
    "nocache": "^3.0.3",
    "notifications-node-client": "^5.1.0",
    "nunjucks": "^3.2.3",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "pg": "^8.7.3",
    "querystring": "^0.2.0",
    "superagent": "^7.1.2"
  },
  "devDependencies": {
    "cypress": "^9.5.4",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-html-reporter": "^3.4.2",
    "jest-junit": "^13.1.0",
    "lint-staged": "^12.3.7",
    "mocha": "^9.2.2",
    "mocha-junit-reporter": "^2.0.2",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "supertest": "^6.2.2"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
