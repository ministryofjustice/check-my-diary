{
    "name": "check-my-diary",
    "version": "0.0.1",
    "description": "HMPPS check my diary",
    "repository": "git@github.com:ministryofjustice/check-my-diary.git",
    "license": "MIT",
    "scripts": {
        "start": "node server.js",
        "start:dev": "npm build && DEBUG=gov-starter-server* nodemon server.js | bunyan -o short",
        "build": "npm run css-build",
        "css-build": "./bin/build-css",
        "clean": "rm -rf assets/* .port.tmp *.log build/* uploads/* test-results.xml",
        "lint": "eslint . --cache --max-warnings 0",
        "test": "jest",
        "migrate": "knex migrate:latest",
        "record-build-info": "node ./bin/record-build-info",
        "security_audit": "npx audit-ci --config audit-ci.json"
    },
    "engines": {
        "node": ">=10.15.3",
        "npm": ">=6.4.1"
    },
    "dependencies": {
        "agentkeepalive": "^4.0.2",
        "applicationinsights": "^1.7.3",
        "applicationinsights-native-metrics": "0.0.5",
        "audit-ci": "^2.5.1",
        "axios": "^0.19.0",
        "body-parser": "^1.19.0",
        "bunyan": "^1.8.12",
        "bunyan-format": "^0.2.1",
        "bunyan-request-logger": "^2.1.0",
        "compression": "^1.7.4",
        "connect-flash": "^0.1.1",
        "cookie-parser": "^1.4.4",
        "cookie-session": "^2.0.0-beta.3",
        "csurf": "^1.10.0",
        "debug": "^4.1.1",
        "dotenv": "^6.2.0",
        "ejs": "2.6.1",
        "express": "^4.17.1",
        "express-request-id": "^1.4.1",
        "express-validator": "^6.3.1",
        "govuk-elements-sass": "^3.1.3",
        "govuk-frontend": "^2.12.0",
        "helmet": "^3.21.1",
        "http-errors": "~1.6.3",
        "ip-range-check": "^0.2.0",
        "joi": "^14.3.1",
        "joi-date-extensions": "^1.2.0",
        "joi-postcode": "^3.0.0",
        "jsonwebtoken": "^8.3.0",
        "jwt-decode": "^2.2.0",
        "knex": "^0.20.10",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "node-sass": "^4.13.0",
        "node-sass-middleware": "0.11.0",
        "notifications-node-client": "^4.5.1",
        "nunjucks": "^3.2.1",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "passport-oauth2": "^1.5.0",
        "pg": "^7.11.0",
        "querystring": "^0.2.0",
        "sass-middleware": "^0.0.3",
        "superagent": "^5.1.0"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^2.4.0",
        "jest": "^25.1.0",
        "jest-html-reporter": "^2.5.0",
        "nock": "^10.0.6",
        "prettier": "^1.18.2",
        "supertest": "^3.4.2"
    }
