{
  "name": "check-my-diary",
  "version": "0.0.1",
  "description": "HMPPS check my diary",
  "repository": "git@github.com:ministryofjustice/check-my-diary.git",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm run build && DEBUG=gov-starter-server* nodemon server.js | bunyan -o short",
    "start-feature": "NODE_ENV=development node server.js | bunyan -o short",
    "compile-sass": "sass --no-source-map --load-path=. ./assets/sass/application.sass:./assets/stylesheets/application.css ./assets/sass/application-ie8.sass:./assets/stylesheets/application-ie8.css --style compressed",
    "watch-sass": "npm run compile-sass -- --watch",
    "build": "npm run compile-sass",
    "clean": "rm -rf node_modules .port.tmp *.log build uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "TZ=UTC jest --config=jest.config.json",
    "int-test": "cypress run",
    "int-test-ui": "cypress open",
    "migrate": "knex migrate:latest",
    "record-build-info": "node ./bin/record-build-info",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "test-mocks": "jest --config=jest.config.integration.json",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "lint-staged": {
    "*.{ts,js,css}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^1.4.0",
    "agentkeepalive": "^4.2.1",
    "applicationinsights": "^2.3.1",
    "audit-ci": "^6.2.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "csurf": "^1.11.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "ejs": "3.1.7",
    "express": "^4.18.0",
    "express-validator": "^6.14.0",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^4.0.1",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "ip-range-check": "^0.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "knex": "^2.0.0",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "nocache": "^3.0.3",
    "notifications-node-client": "^5.1.1",
    "nunjucks": "^3.2.3",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "pg": "^8.7.3",
    "querystring": "^0.2.0",
    "redis": "^4.1.0",
    "superagent": "^7.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/bunyan-format": "^0.2.4",
    "@types/compression": "^1.7.2",
    "@types/connect-flash": "^0.0.37",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-session": "^2.0.44",
    "@types/csurf": "^1.11.2",
    "@types/express-session": "^1.17.4",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.33",
    "@types/nunjucks": "^3.2.1",
    "@types/passport": "^1.0.7",
    "@types/passport-oauth2": "^1.4.11",
    "@types/superagent": "^4.1.15",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cypress": "^9.6.0",
    "cypress-multi-reporters": "^1.6.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-html-reporter": "^3.5.0",
    "jest-junit": "^13.2.0",
    "lint-staged": "^12.4.1",
    "mocha": "^9.2.2",
    "mocha-junit-reporter": "^2.0.2",
    "nock": "^13.2.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "supertest": "^6.2.3",
    "typescript": "^4.6.4"
  }
}
