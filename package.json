{
  "name": "check-my-diary",
  "version": "0.0.1",
  "description": "HMPPS check my diary",
  "repository": "git@github.com:ministryofjustice/check-my-diary.git",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm run build && DEBUG=gov-starter-server* nodemon server.js | bunyan -o short",
    "start-feature": "NODE_ENV=development node server.js | bunyan -o short",
    "build": "npm run css-build",
    "css-build": "./bin/build-css",
    "clean": "rm -rf assets/* .port.tmp *.log build/* uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "TZ=UTC jest --config=jest.config.json",
    "int-test": "cypress run",
    "int-test-ui": "cypress open",
    "migrate": "knex migrate:latest",
    "record-build-info": "node ./bin/record-build-info",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "test-mocks": "jest --config=jest.config.integration.json"
  },
  "engines": {
    "node": ">=14.*",
    "npm": ">=7.11.0"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^0.2.5",
    "agentkeepalive": "^4.1.4",
    "applicationinsights": "^2.1.5",
    "audit-ci": "^3.1.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^2.0.0-beta.3",
    "csurf": "^1.10.0",
    "debug": "^4.3.2",
    "dotenv": "^6.2.0",
    "ejs": "3.1.6",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "express-validator": "^6.6.1",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^3.13.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.8.0",
    "ip-range-check": "^0.2.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^3.1.2",
    "knex": "^0.95.6",
    "moment": "^2.29.0",
    "morgan": "^1.9.1",
    "nocache": "^3.0.1",
    "notifications-node-client": "^5.1.0",
    "nunjucks": "^3.2.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "pg": "^8.6.0",
    "querystring": "^0.2.0",
    "sass-middleware": "^0.0.3",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "cypress": "^8.7.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.1",
    "jest": "^27.3.1",
    "jest-html-reporter": "^3.3.0",
    "jest-junit": "^13.0.0",
    "lint-staged": "^11.2.5",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "nock": "^13.0.11",
    "nodemon": "^2.0.4",
    "node-sass": "^6.0.1",
    "prettier": "~2.2.1",
    "supertest": "^6.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
